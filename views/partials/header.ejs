<header class="top-header">
  <span class="siteName">
    <a href="/" title="Return to home page">CSE Motors</a>
  </span>
  <div class="logout-btn">
    <% if (locals.loggedin) { %>
      <a title="Click to view account" href="/account/">
        Welcome <%= locals.accountData.account_firstname %>
      </a>

      <a title="Click to logout" href="/account/logout">Logout</a>
    <% } else { %>
      <a title="Click to login" href="/account/login">Login</a>
      <% } %>
  </div>
</header>
